{
  "name": "nextjs-fe-hosting",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "next dev --experimental-https",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "cms:login": "npx @optimizely/cms-cli@latest login",
    "cms:push-config": "npx @optimizely/cms-cli@latest config push ./optimizely.config.mjs",
    "cms:push-config-force": "npx @optimizely/cms-cli@latest config push ./optimizely.config.mjs --force",
    "cms:pull-config": "npx @optimizely/cms-cli@latest config pull --output ./content-types.json"
  },
  "dependencies": {
    "@optimizely/cms-sdk": "^1.0.0",
    "@tailwindcss/typography": "^0.5.19",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
